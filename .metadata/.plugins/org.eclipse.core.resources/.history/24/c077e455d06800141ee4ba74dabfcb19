package com.pracownia5;

public class Lista
{
	public Lista()
	{
		m_poczatek = null;
		m_koniec   = null;
	}
	
	public void wstaw(Object p_wartosc)
	{
		Wezel l_wezel = new Wezel(null, null, p_wartosc);
		if (isEmpty())
		{
			m_poczatek = l_wezel;
			m_koniec   = l_wezel;
		}
		else
		{
			Wezel l_temp = m_poczatek;
			while (l_temp != null)
			{
				if (l_temp.getNastepny() == null)
				{
					l_temp.setNastepny(l_wezel);
				}
			}
		}
	}
	
	public boolean isEmpty()
	{
		return m_poczatek == null;
	}
	
	
	
	Wezel m_poczatek;
	Wezel m_koniec;
}
