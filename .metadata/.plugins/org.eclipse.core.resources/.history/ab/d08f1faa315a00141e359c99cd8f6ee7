package com.pracownia3;

public class Arctan extends Operator1Arg
{
	public Arctan(Wyrazenie p_argument)
	{
		super(p_argument);
	}

	@Override
	public double oblicz()
	{
		return Math.atan(m_argumentPrawy.oblicz());
	}
	
	@Override
	public String toString()
	{
		return "arctan(" + m_argumentPrawy.oblicz() + ")";
	}
	
	@Override
	public boolean equals(Wyrazenie p_wyrazenie)
	{
		return this.oblicz() == p_wyrazenie.oblicz();
	}
}
