package com.pracownia3;

public class Zbior {
	public Zbior()
	{
		m_wielkoscZbioru = 2;
		m_para = new Para[m_wielkoscZbioru];
	}
	
	public void wstaw (String  p_klucz, double p_wartosc)
	{
		m_para = new Para(p_klucz, p_wartosc);
	}
	
	public double czytaj(String p_klucz)  throws IllegalArgumentException
	{
		if(m_para.m_klucz != p_klucz)
		{
			throw new IllegalArgumentException();
		}
		return m_para.getWartosc();
	}
	
	
	private Para[] m_para;
	private int m_wielkoscZbioru;
}