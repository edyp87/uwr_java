package com.pracownia3;

public class Zbior {
	public Zbior()
	{
		m_wielkoscZbioru = 2;
	}
	
	public void wstaw (String  p_klucz, double p_wartosc) throws IllegalArgumentException
	{
		m_para = new Para(p_klucz, p_wartosc);
	}
	
	public double czytaj(String p_klucz)
	{
		return m_para.getWartosc();
	}
	
	
	private Para m_para;
	private int m_wielkoscZbioru;
}